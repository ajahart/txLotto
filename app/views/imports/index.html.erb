<h1>Listing imports</h1>

<table>
  <tr>
    <th>Gamenumber</th>
    <th>Gamename</th>
    <th>Closedate</th>
    <th>Ticketprice</th>
    <th>Prizelevel</th>
    <th>Totalprizes</th>
    <th>Prizeclaimed</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @imports.each do |import| %>
  <tr>
    <td><%= import.gameNumber %></td>
    <td><%= import.gameName %></td>
    <td><%= import.closeDate %></td>
    <td><%= import.ticketPrice %></td>
    <td><%= import.prizeLevel %></td>
    <td><%= import.totalPrizes %></td>
    <td><%= import.prizeClaimed %></td>
    <td><%= link_to 'Show', import %></td>
    <td><%= link_to 'Edit', edit_import_path(import) %></td>
    <td><%= link_to 'Destroy', import, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Import', new_import_path %>
